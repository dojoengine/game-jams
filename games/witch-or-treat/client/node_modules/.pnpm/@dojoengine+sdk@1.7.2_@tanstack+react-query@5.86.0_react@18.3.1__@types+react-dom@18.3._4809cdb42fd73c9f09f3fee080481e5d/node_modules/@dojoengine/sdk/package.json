{
    "name": "@dojoengine/sdk",
    "version": "1.7.2",
    "description": "Dojo SDK: Build onchain and provable apps faster",
    "author": "Dojo Team",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/web/index.d.ts",
            "import": "./dist/web/index.js"
        },
        "./state": {
            "types": "./dist/web/state.d.ts",
            "import": "./dist/web/state.js"
        },
        "./react": {
            "types": "./dist/web/react.d.ts",
            "import": "./dist/web/react.js"
        },
        "./sql": {
            "types": "./dist/web/sql.d.ts",
            "import": "./dist/web/sql.js"
        },
        "./experimental": {
            "types": "./dist/web/experimental.d.ts",
            "import": "./dist/web/experimental.js"
        },
        "./types": {
            "types": "./dist/web/index.d.ts",
            "import": "./dist/web/index.d.ts"
        },
        "./node": {
            "types": "./dist/node/index.d.ts",
            "import": "./dist/node/index.js"
        }
    },
    "scripts": {
        "lint:check": "biome lint .",
        "lint": "biome lint . --write",
        "build:deps": "tsup --config tsup.node.ts && tsup --config tsup.web.ts",
        "build": "bun run build:deps",
        "test": "vitest run --config ./vitest.config.ts",
        "coverage": "vitest run --coverage",
        "format:check": "biome format .",
        "format": "biome format . --write",
        "docs": "typedoc"
    },
    "./package.json": "./package.json",
    "devDependencies": {
        "@anymud/bun-plugin-dts": "^1.0.2",
        "@biomejs/biome": "^2.1.3",
        "@dojoengine/internal": "1.6.9",
        "@rollup/plugin-commonjs": "^28.0.2",
        "@types/node": "^22.14.1",
        "@vitest/coverage-v8": "^1.6.1",
        "benchmark": "^2.1.4",
        "bun-plugin-dts": "^0.3.0",
        "eslint": "^8.57.1",
        "lodash": "^4.17.21",
        "prettier": "^2.8.8",
        "tsup": "^8.4.0",
        "typescript": "^5.7.3",
        "vite": "^3.2.11",
        "vite-plugin-wasm": "^3.4.1",
        "vitest": "^1.6.1"
    },
    "peerDependencies": {
        "@tanstack/react-query": "^5.62.16",
        "@types/react": "^18.3.1",
        "@types/react-dom": "^18.3.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "starknet": "^8.1.2"
    },
    "dependencies": {
        "@dojoengine/core": "1.7.1",
        "@dojoengine/grpc": "1.7.2",
        "@dojoengine/state": "1.7.2",
        "@dojoengine/torii-client": "1.7.2",
        "@dojoengine/torii-wasm": "1.7.2",
        "@dojoengine/utils": "1.7.1",
        "@starknet-react/chains": "^5.0.1",
        "@starknet-react/core": "^5.0.1",
        "effect": "^3.17.1",
        "immer": "^10.1.1",
        "neverthrow": "^8.2.0",
        "zustand": "^4.5.6"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dojoengine/sdk.git"
    },
    "bugs": {
        "url": "https://github.com/dojoengine/sdk/issues"
    },
    "homepage": "https://github.com/dojoengine/sdk#readme",
    "keywords": [
        "dojo",
        "sdk",
        "engine",
        "typescript",
        "torii"
    ],
    "engines": {
        "node": ">=18.0.0"
    }
}
