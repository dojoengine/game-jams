#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ak/dojo_games/halloween_game/client/node_modules/.pnpm/@protobuf-ts+plugin@2.11.1/node_modules/@protobuf-ts/plugin/bin/node_modules:/home/ak/dojo_games/halloween_game/client/node_modules/.pnpm/@protobuf-ts+plugin@2.11.1/node_modules/@protobuf-ts/plugin/node_modules:/home/ak/dojo_games/halloween_game/client/node_modules/.pnpm/@protobuf-ts+plugin@2.11.1/node_modules/@protobuf-ts/node_modules:/home/ak/dojo_games/halloween_game/client/node_modules/.pnpm/@protobuf-ts+plugin@2.11.1/node_modules:/home/ak/dojo_games/halloween_game/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ak/dojo_games/halloween_game/client/node_modules/.pnpm/@protobuf-ts+plugin@2.11.1/node_modules/@protobuf-ts/plugin/bin/node_modules:/home/ak/dojo_games/halloween_game/client/node_modules/.pnpm/@protobuf-ts+plugin@2.11.1/node_modules/@protobuf-ts/plugin/node_modules:/home/ak/dojo_games/halloween_game/client/node_modules/.pnpm/@protobuf-ts+plugin@2.11.1/node_modules/@protobuf-ts/node_modules:/home/ak/dojo_games/halloween_game/client/node_modules/.pnpm/@protobuf-ts+plugin@2.11.1/node_modules:/home/ak/dojo_games/halloween_game/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@protobuf-ts/plugin/bin/protoc-gen-ts" "$@"
else
  exec node  "$basedir/../@protobuf-ts/plugin/bin/protoc-gen-ts" "$@"
fi
